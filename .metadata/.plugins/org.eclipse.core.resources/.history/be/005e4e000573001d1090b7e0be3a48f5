package school;

public class Score {
	int studentId;
	Subject subject;
	int point;
	
	Score(int studentId, Subject subject, int point) {
		this.studentId = studentId;
		this.subject = subject;
		this.point = point;
	}

	
	@Override
	public String toString() {
		return "학번: " + this.studentId + "," + subject.getSubjectName() + ":" + point;
	}
	
	
}
